.round-bar {
    display: flex;
    flex-wrap: wrap;
}
.round-item {
    flex: 0 0 33%;
    height: 250rpx;
    position: relative;
}
.round-box {
    width: 150rpx;
    height: 150rpx;
    line-height: 150rpx;
    text-align: center;
    border: 2rpx red solid;
    border-radius: 10rpx;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    margin: auto;
}
.lock {
    position: absolute;
    top: -50%;
    left: -25%;
    font-size: 70rpx;
}
.star-bar {
    display: flex;
    position: absolute;
    width: 100%;
    bottom: 50rpx;
    height: 50rpx;
}
.star-item {
    flex: 1;
}